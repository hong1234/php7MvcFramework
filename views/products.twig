{% extends 'layout.twig' %}

{% block title %}
    Products
{% endblock %}

{% block content %}
<table>
    <thead>
        <th>Id</th>
        <th>Product</th>
    </thead>
{% for item in result %}
    <tr>
        <td>{{ item.id }}</td>
        <td>{{ item.name }}</td>
    </tr>
{% endfor %}
</table>
{% endblock %}